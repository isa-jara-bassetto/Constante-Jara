      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# --- CONSTANTES JARA ---\n",
        "rho_j = 1.44e-18  # Densidade do vácuo Jara (kg/m^3)\n",
        "c = 3e8           # Velocidade da luz (m/s)\n",
        "\n",
        "# --- ESCALAS DE DISTÂNCIA (Anos-luz para metros) ---\n",
        "distancias_ly = np.array([1, 10, 100, 1e6, 1e9, 13.8e9])\n",
        "distancias_m = distancias_ly * 9.461e15\n",
        "\n",
        "# --- CÁLCULO DO ATRASO (Spectral Lag) ---\n",
        "# O atraso aumenta conforme a luz atravessa a densidade rho_j\n",
        "atraso = (rho_j * distancias_m) / c\n",
        "\n",
        "# --- GRÁFICO 1: SPECTRAL LAG ---\n",
        "plt.figure(figsize=(10, 5))\n",
        "plt.plot(distancias_ly, atraso, 'o-', color='blue', label='Efeito Jara')\n",
        "plt.xscale('log')\n",
        "plt.yscale('log')\n",
        "plt.title('Atraso da Luz pela Densidade Jara')\n",
        "plt.xlabel('Distância (Anos-luz)')\n",
        "plt.ylabel('Atraso Estimado (segundos)')\n",
        "plt.grid(True, which=\"both\", ls=\"-\", alpha=0.5)\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "print(f\"Atraso em 13.8 bilhões de anos-luz: {atraso[-1]:.4f} segundos\")"
      ]
    }
  ]
